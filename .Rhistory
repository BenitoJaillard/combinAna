wasd <- function(x, w, na.rm = TRUE) {
if (na.rm == TRUE) {
index <- !(is.na(x) | is.na(w))
x     <- x[index]
w     <- w[index]
}
return(sqrt(ifelse(length(x) > 0,
sum( w * ((x - sum(x*w)/sum(w)) ^ 2) ) / sum(w),
NA)
)
}
